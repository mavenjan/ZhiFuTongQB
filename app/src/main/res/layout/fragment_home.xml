<?xml version="1.0" encoding="utf-8"?><android.support.design.widget.CoordinatorLayout    xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    android:id="@+id/cdt_help_daily"    android:layout_width="match_parent"    android:layout_height="match_parent">    <LinearLayout        android:layout_width="match_parent"        android:layout_height="73dp"        android:background="@color/colorPrimary"        android:orientation="horizontal">        <TextView            android:id="@+id/tv_searcher"            android:layout_width="0dp"            android:layout_height="match_parent"            android:layout_gravity="center_vertical"            android:layout_marginLeft="8dp"            android:layout_marginTop="8dp"            android:layout_weight="1"            android:gravity="center|left"            android:paddingLeft="10dp"            android:text="致富通"            android:textColor="@color/white"            android:textSize="22sp"/>        <LinearLayout            android:id="@+id/ll_weather"            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:orientation="horizontal">            <ImageView                android:id="@+id/iv_weather"                android:layout_width="40dp"                android:layout_height="40dp"                android:layout_gravity="center"                android:layout_marginTop="8dp"                android:padding="2dp"                android:scaleType="centerInside"                android:src="@drawable/biz_plugin_weather_baoxue"/>            <TextView                android:id="@+id/tv_location"                android:layout_width="wrap_content"                android:layout_height="wrap_content"                android:text="许昌市"                android:textColor="@color/white"                android:visibility="gone"/>            <LinearLayout                android:layout_width="wrap_content"                android:layout_height="30dp"                android:layout_gravity="center"                android:layout_marginTop="8dp"                android:orientation="vertical"                android:paddingRight="8dp">                <TextView                    android:id="@+id/tv_real_time_temperature"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_gravity="center"                    android:text="15℃"                    android:textColor="@color/white"                    android:textSize="12sp"/>                <TextView                    android:id="@+id/tv_temperature"                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_gravity="center"                    android:text="-1℃~8℃"                    android:textColor="@color/white"                    android:textSize="12sp"/>            </LinearLayout>        </LinearLayout>    </LinearLayout>    <LinearLayout        android:layout_width="match_parent"        android:layout_height="100dp"        android:layout_marginTop="73dp"        android:background="@color/colorPrimary"        android:orientation="horizontal"        android:padding="12dp">        <TextView            android:id="@+id/tv_help_task"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="1"            android:background="@drawable/button_bg_primary"            android:drawablePadding="6dp"            android:drawableTop="@drawable/notify_work_task"            android:gravity="center"            android:text="@string/help_task"            android:textColor="@color/white"/>        <TextView            android:id="@+id/tv_daily_list"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="1"            android:background="@drawable/button_bg_primary"            android:drawablePadding="6dp"            android:drawableTop="@drawable/notify_daily_record"            android:gravity="center"            android:text="@string/daily_record"            android:textColor="@color/white"/>        <TextView            android:id="@+id/tv_helper_result"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="1"            android:background="@drawable/button_bg_primary"            android:drawablePadding="6dp"            android:drawableTop="@drawable/notify_effect"            android:gravity="center"            android:text="@string/poverty_alleviation_effect"            android:textColor="@color/white"/>        <TextView            android:id="@+id/tv_sign_in"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="1"            android:background="@drawable/button_bg_primary"            android:drawablePadding="6dp"            android:drawableTop="@drawable/notify_sign_in"            android:gravity="center"            android:text="@string/sign_in"            android:textColor="@color/white"/>    </LinearLayout>    <android.support.v4.widget.SwipeRefreshLayout        android:id="@+id/swipe_container"        android:layout_width="match_parent"        android:layout_height="match_parent"        android:layout_marginTop="173dp"        android:background="@color/common_bg">        <ScrollView            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:scrollbars="none">            <LinearLayout                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:orientation="vertical">                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="50dp"                    android:background="@color/colorPrimary"                    android:orientation="horizontal"                    android:visibility="gone">                    <TextView                        android:id="@+id/tv_searcher1"                        android:layout_width="0dp"                        android:layout_height="30dp"                        android:layout_gravity="center_vertical"                        android:layout_marginLeft="8dp"                        android:layout_marginTop="8dp"                        android:layout_weight="1"                        android:background="@color/half_transparent"                        android:gravity="center|left"                        android:paddingLeft="10dp"                        android:text="输入查询关键字"                        android:textColor="@color/white"/>                    <LinearLayout                        android:id="@+id/ll_weather1"                        android:layout_width="wrap_content"                        android:layout_height="match_parent"                        android:orientation="horizontal">                        <ImageView                            android:id="@+id/iv_weather1"                            android:layout_width="30dp"                            android:layout_height="30dp"                            android:layout_gravity="center"                            android:layout_marginTop="8dp"                            android:padding="2dp"                            android:scaleType="centerInside"                            android:src="@drawable/biz_plugin_weather_baoxue"/>                        <TextView                            android:id="@+id/tv_location1"                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:text="许昌市"                            android:textColor="@color/white"                            android:visibility="gone"/>                        <LinearLayout                            android:layout_width="wrap_content"                            android:layout_height="30dp"                            android:layout_gravity="center"                            android:layout_marginTop="8dp"                            android:orientation="vertical"                            android:paddingRight="8dp">                            <TextView                                android:id="@+id/tv_real_time_temperature1"                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_gravity="center"                                android:text="15℃"                                android:textColor="@color/white"                                android:textSize="12sp"/>                            <TextView                                android:id="@+id/tv_temperature1"                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_gravity="center"                                android:text="-1℃~8℃"                                android:textColor="@color/white"                                android:textSize="12sp"/>                        </LinearLayout>                    </LinearLayout>                </LinearLayout>                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:background="@color/white"                    android:orientation="horizontal"                    android:padding="8dp"                    android:visibility="gone">                    <TextView                        android:id="@+id/tv_channel_news"                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="1"                        android:background="@drawable/button_bg_common"                        android:drawablePadding="6dp"                        android:drawableTop="@drawable/ic_channel_news"                        android:gravity="center"                        android:text="@string/poverty_alleviation_news"                        android:textColor="@color/primary_text"/>                    <TextView                        android:id="@+id/tv_channel_offer"                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="1"                        android:background="@drawable/button_bg_common"                        android:drawablePadding="6dp"                        android:drawableTop="@drawable/ic_channel_offer"                        android:gravity="center"                        android:text="@string/poverty_alleviation_offer"                        android:textColor="@color/primary_text"/>                    <TextView                        android:id="@+id/tv_channel_project"                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="1"                        android:background="@drawable/button_bg_common"                        android:drawablePadding="6dp"                        android:drawableTop="@drawable/ic_channel_project"                        android:gravity="center"                        android:text="@string/poverty_alleviation_project"                        android:textColor="@color/primary_text"/>                    <TextView                        android:id="@+id/tv_channel_teach"                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="1"                        android:background="@drawable/button_bg_common"                        android:drawablePadding="6dp"                        android:drawableTop="@drawable/ic_channel_teach"                        android:gravity="center"                        android:text="@string/poverty_alleviation_knowledge"                        android:textColor="@color/primary_text"/>                </LinearLayout>                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:background="@color/white"                    android:orientation="vertical">                    <View                        android:layout_width="match_parent"                        android:layout_height="1dp"                        android:layout_marginLeft="18dp"                        android:layout_marginRight="18dp"                        android:background="@color/common_bg"/>                </LinearLayout>                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:background="@color/white"                    android:orientation="horizontal"                    android:padding="8dp"                    android:visibility="gone">                    <TextView                        android:id="@+id/tv_channel_unit"                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="1"                        android:background="@drawable/button_bg_common"                        android:drawablePadding="6dp"                        android:drawableTop="@drawable/ic_channel_unit"                        android:gravity="center"                        android:text="@string/poverty_alleviation_unit"                        android:textColor="@color/primary_text"/>                    <TextView                        android:id="@+id/tv_channel_org"                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="1"                        android:background="@drawable/button_bg_common"                        android:drawablePadding="6dp"                        android:drawableTop="@drawable/ic_channel_org"                        android:gravity="center"                        android:text="@string/poverty_alleviation_org"                        android:textColor="@color/primary_text"/>                    <TextView                        android:id="@+id/tv_channel_money"                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="1"                        android:background="@drawable/button_bg_common"                        android:drawablePadding="6dp"                        android:drawableTop="@drawable/ic_channel_money"                        android:gravity="center"                        android:text="@string/poverty_alleviation_money"                        android:textColor="@color/primary_text"/>                    <TextView                        android:id="@+id/tv_channel_tel"                        android:layout_width="0dp"                        android:layout_height="wrap_content"                        android:layout_weight="1"                        android:background="@drawable/button_bg_common"                        android:drawablePadding="6dp"                        android:drawableTop="@drawable/ic_channel_phone"                        android:gravity="center"                        android:text="@string/poverty_alleviation_phone"                        android:textColor="@color/primary_text"/>                </LinearLayout>                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:layout_marginTop="6dp"                    android:background="@color/white"                    android:orientation="vertical">                    <LinearLayout                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:orientation="horizontal">                        <TextView                            android:layout_width="0dp"                            android:layout_height="wrap_content"                            android:layout_weight="5"                            android:padding="6dp"                            android:paddingLeft="21dp"                            android:text="帮扶对象"                            android:textColor="@color/colorPrimary"                            android:textSize="16sp"/>                        <TextView                            android:id="@+id/tv_object_more"                            android:layout_width="0dp"                            android:layout_height="wrap_content"                            android:layout_gravity="right"                            android:layout_weight="1"                            android:gravity="center"                            android:padding="6dp"                            android:paddingLeft="20dp"                            android:text="更多"                            android:textColor="@color/colorAccent"                            android:textSize="16sp"/>                    </LinearLayout>                    <android.support.v7.widget.RecyclerView                        android:id="@+id/rv_person"                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:layout_gravity="center"                        android:paddingBottom="6dp"                        android:paddingLeft="8dp"                        android:paddingRight="8dp"                        android:paddingTop="4dp">                    </android.support.v7.widget.RecyclerView>                </LinearLayout>                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="match_parent"                    android:layout_marginTop="6dp"                    android:background="@color/white"                    android:orientation="vertical">                    <TextView                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:padding="6dp"                        android:paddingLeft="21dp"                        android:text="帮扶日志"                        android:textColor="@color/colorPrimary"                        android:textSize="16sp"/>                    <ListView                        android:id="@+id/listview_common"                        android:layout_width="match_parent"                        android:layout_height="match_parent"                        android:divider="@color/white"                        android:dividerHeight="1dp"                        android:scrollbars="none"/>                </LinearLayout>            </LinearLayout>        </ScrollView>    </android.support.v4.widget.SwipeRefreshLayout>    <android.support.v4.view.ViewPager        android:id="@+id/detail_view"        android:layout_width="match_parent"        android:layout_height="match_parent"        android:background="@color/white"        android:contentDescription="用来展示图片的viewpager"        android:gravity="center"        android:visibility="invisible"/></android.support.design.widget.CoordinatorLayout>