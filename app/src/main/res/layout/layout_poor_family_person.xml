                    android:textColor="@color/primary_text"
                    android:textSize="16sp"/>

                <EditText
                    android:id="@+id/tv_show_working_time"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:layout_marginLeft="8dp"
                    android:background="@null"
                    android:paddingLeft="16dp"
                    android:textColor="@color/primary_text"/>
            </LinearLayout>

            <!--是否现役军人-->
            <LinearLayout
                android:id="@+id/ll_soldiers_on_service"
                style="@style/commonLinearLayout">

                <TextView
                    android:id="@+id/tv_soldiers_on_service"
                    style="@style/commontext"
                    android:text="@string/soldiers_on_service"
                    android:textColor="@color/primary_text"
                    android:textSize="16sp"/>

                <EditText
                    android:id="@+id/tv_show_soldiers_on_service"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:layout_marginLeft="8dp"
                    android:background="@null"
                    android:paddingLeft="16dp"
                    android:textColor="@color/primary_text"/>
            </LinearLayout>

            <!--与户主关系-->
            <LinearLayout
                android:id="@+id/ll_relationship_with_householder"
                style="@style/commonLinearLayout">

                <TextView
                    android:id="@+id/tv_relationship_with_householder"
                    style="@style/commontext"
                    android:text="@string/relationship_with_householder"
                    android:textColor="@color/primary_text"
                    android:textSize="16sp"/>

                <EditText
                    android:id="@+id/tv_show_relationship_with_householder"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:layout_marginLeft="8dp"
                    android:background="@null"
                    android:paddingLeft="16dp"
                    android:textColor="@color/primary_text"/>
            </LinearLayout>

            <!--城乡居民基本医疗保险-->
            <LinearLayout
                android:id="@+id/ll_XinNongHe"
                style="@style/commonLinearLayout">

                <TextView
                    android:id="@+id/tv_XinNongHe"
                    style="@style/commontext"
                    android:text="@string/XinNongHe"
                    android:textColor="@color/primary_text"
                    android:textSize="16sp"/>

                <EditText
                    android:id="@+id/tv_show_XinNongHe"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:layout_marginLeft="8dp"
                    android:background="@null"
                    android:paddingLeft="16dp"
                    android:textColor="@color/primary_text"/>
            </LinearLayout>

            <!--城乡居民社会养老保险-->
            <LinearLayout
                android:id="@+id/ll_Old_age_insurance"
                style="@style/commonLinearLayout">

                <TextView
                    android:id="@+id/tv_Old_age_insurance"
                    style="@style/commontext"
                    android:text="@string/Old_age_insurance"
                    android:textColor="@color/primary_text"
                    android:textSize="16sp"/>

                <EditText
                    android:id="@+id/tv_show_Old_age_insurance"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:layout_marginLeft="8dp"
                    android:background="@null"
                    android:paddingLeft="16dp"
                    android:textColor="@color/primary_text"/>
            </LinearLayout>

            <!--城镇职工基本养老保险-->
            <LinearLayout
                android:id="@+id/ll_ChengZhenBaoXian"
                style="@style/commonLinearLayout">

                <TextView
                    android:id="@+id/tv_ChengZhenBaoXian"
                    style="@style/commontext"
                    android:text="@string/ChengZhenBaoXian"
                    android:textColor="@color/primary_text"
                    android:textSize="16sp"/>

                <EditText
                    android:id="@+id/et_show_ChengZhenBaoXian"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:layout_marginLeft="8dp"
                    android:background="@null"
                    android:paddingLeft="16dp"
                    android:textColor="@color/primary_text"/>
            </LinearLayout>

            <!--大病保险-->
            <LinearLayout
                android:id="@+id/ll_DaBingBaoXian"
                style="@style/commonLinearLayout">

                <TextView
                    android:id="@+id/tv_DaBingBaoXian"
                    style="@style/commontext"
                    android:text="@string/DaBingBaoXian"
                    android:textColor="@color/primary_text"
                    android:textSize="16sp"/>

                <EditText
                    android:id="@+id/tv_show_DaBingBaoXian"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:layout_marginLeft="8dp"
                    android:background="@null"
                    android:paddingLeft="16dp"
                    android:textColor="@color/primary_text"/>
            </LinearLayout>

        </LinearLayout>
    </ScrollView>
</LinearLayout>